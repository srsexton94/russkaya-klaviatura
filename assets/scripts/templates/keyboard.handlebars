      <li id="KeyY" class="letter">{{keys.[18]}}</li>
      <li id="KeyU" class="letter">{{keys.[19]}}</li>
      <li id="KeyI" class="letter">{{keys.[20]}}</li>
      <li id="KeyO" class="letter">{{keys.[21]}}</li>
      <li id="KeyP" class="letter">{{keys.[22]}}</li>
      <li id="BracketLeft" class="symbol">{{keys.[23]}}</li>
      <li id="BracketRight" class="symbol">{{keys.[24]}}</li>
      <li id="Backslash" class="symbol">{{keys.[25]}}</li>
      <li id="CapsLock" class="capslock">заглушки</li>
      <li id="KeyA" class="letter">{{keys.[26]}}</li>
      <li id="KeyS" class="letter">{{keys.[27]}}</li>
      <li id="KeyD" class="letter">{{keys.[28]}}</li>
      <li id="KeyF" class="letter">{{keys.[29]}}</li>
