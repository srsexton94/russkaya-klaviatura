      <li id="Quote" class="letter">{{keys.[36]}}</li>
      <li id="Enter" class="return">новая линия</li>
      <li id="ShiftLeft" class="shift">капитал.</li>
      <li id="KeyZ" class="letter">{{keys.[37]}}</li>
      <li id="KeyX" class="letter">{{keys.[38]}}</li>
      <li id="KeyC" class="letter">{{keys.[39]}}</li>
      <li id="KeyV" class="letter">{{keys.[40]}}</li>
      <li id="KeyB" class="letter">{{keys.[41]}}</li>
      <li id="KeyN" class="letter">{{keys.[42]}}</li>
