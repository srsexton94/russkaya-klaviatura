{{#with keys as |keys|}}
  <textarea id="write" class="screen" rows="6" cols="60"></textarea>
  <ul id="keyboard" class="keyboard">
      <li id="Backquote" class="symbol">{{keys.[0]}}</li>
      <li id="Digit1" class="symbol">{{keys.[1]}}</li>
      <li id="Digit2" class="symbol">{{keys.[2]}}</li>
      <li id="Digit3" class="symbol">{{keys.[3]}}</li>
      <li id="Digit4" class="symbol">{{keys.[4]}}</li>
      <li id="Digit5" class="symbol">{{keys.[5]}}</li>
      <li id="Digit6" class="symbol">{{keys.[6]}}</li>
      <li id="Digit7" class="symbol">{{keys.[7]}}</li>
      <li id="Digit8" class="symbol">{{keys.[8]}}</li>
      <li id="Digit9" class="symbol">{{keys.[9]}}</li>
      <li id="Digit0" class="symbol">{{keys.[10]}}</li>
      <li id="Minus" class="symbol">{{keys.[11]}}</li>
      <li id="Equal" class="symbol">{{keys.[12]}}</li>
      <li id="Backspace" class="delete">удалять</li>
      <li id="Tab" class="tab">табуляция</li>
      <li id="KeyQ" class="letter">{{keys.[13]}}</li>
      <li id="KeyW" class="letter">{{keys.[14]}}</li>
      <li id="KeyE" class="letter">{{keys.[15]}}</li>
      <li id="KeyR" class="letter">{{keys.[16]}}</li>
